{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true,
		"jest": true
	},
	"extends": "airbnb-base",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": "off",
		"indent": [2, "tab"],
		"no-tabs": 0,
		"no-underscore-dangle": "off"
	}
}
